{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS=--max_old_space_size=4096 next dev",
    "dev-windows": "set NODE_OPTIONS=--max_old_space_size=4096 && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test-backend": "jest --testPathPattern=__tests__/backend --watch",
    "test-backend:ci": "jest --testPathPattern=__tests__/backend --ci",
    "test-frontend": "jest --testPathPattern=__tests__/frontend --watch",
    "test-frontend:ci": "jest --testPathPattern=__tests__/frontend --ci"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@fullcalendar/core": "^6.1.5",
    "@fullcalendar/daygrid": "^6.1.5",
    "@fullcalendar/interaction": "^6.1.5",
    "@fullcalendar/react": "^6.1.5",
    "@fullcalendar/timegrid": "^6.1.5",
    "@mantine/core": "^5.10.3",
    "@mantine/dates": "^5.10.3",
    "@mantine/form": "^5.10.3",
    "@mantine/hooks": "^5.10.3",
    "@mantine/next": "^5.10.3",
    "@mantine/notifications": "^5.10.3",
    "@next/font": "13.1.6",
    "@react-latest-ui/react-sticky-notes": "^0.2.6",
    "@supabase/auth-helpers-nextjs": "^0.5.4",
    "@supabase/auth-ui-react": "^0.3.5",
    "@supabase/supabase-js": "^2.13.1",
    "@tabler/icons-react": "^2.16.0",
    "dayjs": "^1.11.7",
    "googleapis": "^111.0.0",
    "next": "13.1.6",
    "openai": "^3.2.1",
    "react": "18.2.0",
    "react-confetti": "^6.1.0",
    "react-dom": "18.2.0",
    "react-spinners": "^0.13.8",
    "react-use": "^17.4.0",
    "server-only": "^0.0.1",
    "tabler-icons-react": "^1.56.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.9.0",
    "dotenv": "^16.0.3",
    "encoding": "^0.1.13",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "pg": "^8.9.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "react-test-renderer": "^18.2.0",
    "supabase": "^1.37.1",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.5"
  },
  "prettier": {
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "importOrder": [
      "^react",
      "^next",
      "^@",
      "^#",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
}
